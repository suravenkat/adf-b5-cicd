{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_dataflowSQL1",
						"type": "DatasetReference"
					},
					"name": "source1Employess"
				},
				{
					"dataset": {
						"referenceName": "ds_dataflowSQL2",
						"type": "DatasetReference"
					},
					"name": "source1Employeesinfo"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_taregtDataflowsCSV",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "join1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          id as integer,",
				"          name as string,",
				"          dep as string,",
				"          salary as decimal(10,2),",
				"          last_updated as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1Employess",
				"source(output(",
				"          id as integer,",
				"          location as string,",
				"          mobilenum as string,",
				"          address as string,",
				"          last_updated as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1Employeesinfo",
				"source1Employess, source1Employeesinfo join(source1Employess@id == source1Employeesinfo@id,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['empinfo'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          empid = source1Employess@id,",
				"          name,",
				"          dep,",
				"          salary,",
				"          last_updated = source1Employess@last_updated,",
				"          id = source1Employeesinfo@id,",
				"          location,",
				"          mobilenum,",
				"          address,",
				"          last_updated = source1Employeesinfo@last_updated",
				"     )) ~> sink1"
			]
		}
	}
}